<div class="container" align="center">
<h1>Pieces</h1>
  
  <%= button_to "Upload a Piece", new_piece_path, :method => :get %> 
  <br>
  
  
  <% @pieces.each do |piece| %>
    <%= image_tag piece.photo.url(:small) %>
    <%= piece.piece_name %>
    <%= piece.artist_id %>
    <%= piece.category %>
    <%= link_to "Delete Piece", piece_path(piece), :method => :delete %>
  <br>
<% end %>